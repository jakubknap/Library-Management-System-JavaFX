<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="500.0" prefWidth="1000.0"
            xmlns="http://javafx.com/javafx/17" fx:controller="pl.knap.libsma.controllers.AddUserController">
    <children>
        <Label layoutX="29.0" layoutY="24.0" text="%addUser.login"/>
        <Label layoutX="30.0" layoutY="53.0" text="%addUser.password"/>
        <Label layoutX="30.0" layoutY="84.0" text="%addUser.type"/>
        <TextField fx:id="loginInput" layoutX="156.0" layoutY="20.0"/>
        <ComboBox fx:id="typeAccountComboBox" layoutX="157.0" layoutY="80.0" prefWidth="150.0"/>
        <Button layoutX="30.0" layoutY="142.0" mnemonicParsing="false" onAction="#addUser" text="%addUserButton"/>
        <PasswordField fx:id="passwordInput" layoutX="157.0" layoutY="49.0"/>
        <Button layoutX="200.0" layoutY="142.0" mnemonicParsing="false" onAction="#backToMenu" text="%back"/>
    </children>
</AnchorPane>
